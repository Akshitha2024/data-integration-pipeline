{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nycdatafactory10"
		},
		"ls_data_lake_gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_data_lake_gen2'"
		},
		"ls_sqldb_nycpayroll_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sqldb_nycpayroll'"
		},
		"ls_data_lake_gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://nycstorageaccount10.dfs.core.windows.net/"
		},
		"ls_sqldb_nycpayroll_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "nyc10.database.windows.net"
		},
		"ls_sqldb_nycpayroll_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db_nycpayroll"
		},
		"ls_sqldb_nycpayroll_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "nycserveradmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "nycdatafactory10_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_data_lake_gen2_accountKey": {
						"value": "[parameters('ls_data_lake_gen2_accountKey')]"
					},
					"ls_sqldb_nycpayroll_password": {
						"value": "[parameters('ls_sqldb_nycpayroll_password')]"
					},
					"ls_data_lake_gen2_properties_typeProperties_url": {
						"value": "[parameters('ls_data_lake_gen2_properties_typeProperties_url')]"
					},
					"ls_sqldb_nycpayroll_properties_typeProperties_server": {
						"value": "[parameters('ls_sqldb_nycpayroll_properties_typeProperties_server')]"
					},
					"ls_sqldb_nycpayroll_properties_typeProperties_database": {
						"value": "[parameters('ls_sqldb_nycpayroll_properties_typeProperties_database')]"
					},
					"ls_sqldb_nycpayroll_properties_typeProperties_userName": {
						"value": "[parameters('ls_sqldb_nycpayroll_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "nycdatafactory10_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'nycdatafactory10_ArmTemplate_0')]"
			]
		}
	]
}